.material-column {
		width: 32%;
		margin-right: 2%;
		border: 1px solid #333; /* Lighter border for visibility */
		background-color: #121212; /* Slightly lighter than black for contrast */
		color: #FFFFFF;
		}
		.material-column:last-child {
		margin-right: 0;
		}
		.material-column th {
		background-color: #6200EA; /* Primary Color */
		color: #FFFFFF; /* Text on Primary */
		padding: 16px;
		font-size: 20px;
		}
		.material-column td {
		padding: 12px;
		border-bottom: 1px solid #333; /* Adjusted for dark theme */
		}
		.material-column a {
		color: #BB86FC; /* Adjusted for visibility */
		text-decoration: none;
		display: block;
		padding: 8px 0;
		}
		.material-column a:hover {
		background-color: #03DAC6; /* Secondary Color */
		color: #000000; /* Text on Secondary */
		}
